<Layout>:
    canvas: 
        Color:
            rgba: 1,1,1,1
        Rectangle:
            pos: self.pos
            size: self.size
            
<MyPageLayout>:
    page: 2
    border: 120
    swipe_threshold: .4
    
    RelativeLayout:
        canvas: 
            Color:
                rgba: 1,0,1,1
            Rectangle:
                size: self.size
        Button:
            text: 'R1'
            size_hint: .4, .4
            pos: 0, 0
            
    FloatLayout:
        Button:
            text: 'F1'
            size_hint: .3, .3
            pos: 0, 0
            
    BoxLayout:
        Button:
            text: 'B1'
            size_hint: .5, .5
            pos: 0,0
